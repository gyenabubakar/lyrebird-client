{
  "name": "turborepo-basic",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "app",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write '**/*.{ts,tsx,.md}'"
  },
  "devDependencies": {
    "prettier": "^2.5.1",
    "turbo": "*"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
